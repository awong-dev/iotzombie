{
  "name": "iot-zombie",
  "description": "Google cloud functions and firebase implemenation for orchestrating IOT devices",
  "repository": "https://github.com/awong-dev/iotzombie.git",
  "dependencies": {},
  "scripts": {
    "firebase": "firebase",
    "webpack": "webpack --config webpack.config.js",
    "lint:js": "eslint --quiet --ext .js --ext .jsx client",
    "deploy": "node generate-app-yaml.js && gcloud app deploy app.yaml",
    "watch": "npm run firebase -- serve --debug --only functions,hosting & npm run watch:client",
    "watch:client": "BABEL_ENV=hot webpack-dev-server --config webpack-hot.config.js",
    "test": "mocha --compilers js:babel-register --recursive test/**/*.spec.js -t 30000"
  },
  "author": "Albert J. Wong",
  "license": "BSD-2-Clause",
  "keywords": [
    "iot"
  ],
  "private": true,
  "devDependencies": {
    "@material/button": "^0.7.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "css-hot-loader": "^1.3.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "firebase-tools": "^3.12.0",
    "immutability-helper": "^2.4.0",
    "material-components-web": "^0.22.0",
    "mocha": "^4.0.0",
    "node-sass": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "resolve-url": "^0.2.1",
    "sass": "^1.0.0-beta.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "whatwg-fetch": "^2.0.1"
  }
}
