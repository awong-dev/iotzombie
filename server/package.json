{
  "name": "iot-zombie",
  "version": "1.0.0",
  "description": "Simple node.js server for orchtestrating IOT devices.",
  "repository": "https://github.com/awong-dev/iotzombie.git",
  "private": true,
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "start": "node app.js",
    "lint:js": "eslint --quiet --ext .js --ext .jsx js",
    "monitor": "nodemon app.js & npm run webpack -- --watch",
    "deploy": "gcloud app deploy app.yaml",
    "test": "mocha --compilers js:babel-register --recursive test/**/*.spec.js -t 30000"
  },
  "author": "Albert J. Wong",
  "license": "BSD-2-Clause",
  "keywords": [
    "iot"
  ],
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-basic-auth": "^0.2.3",
    "express-winston": "^2.1.2",
    "lodash": "^4.17.2",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta6",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "foundation-sites": "^6.3.0",
    "mocha": "^3.0.2",
    "node-sass": "^4.1.1",
    "nodejs-repo-tools": "git+https://github.com/GoogleCloudPlatform/nodejs-repo-tools.git#21daa823090c43fb667157c8b5b0c3b7f45a8357",
    "resolve-url": "^0.2.1",
    "sass": "^0.5.0",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.0",
    "webpack": "^1.14.0"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
