{
  "name": "iot-zombie",
  "description": "Google cloud functions and firebase implemenation for orchestrating IOT devices",
  "repository": "https://github.com/awong-dev/iotzombie.git",
  "dependencies": {
    "firebase": "^4.5.0",
    "whatwg-fetch": "^2.0.1"
  },
  "scripts": {
    "firebase": "firebase",
    "webpack": "webpack --config webpack.config.js",
    "lint:js": "eslint --quiet --ext .js --ext .jsx client",
    "deploy": "node generate-app-yaml.js && gcloud app deploy app.yaml",
    "watch": "npm run firebase -- serve --debug --only functions,hosting & npm run webpack -- --watch",
    "test": "mocha --compilers js:babel-register --recursive test/**/*.spec.js -t 30000"
  },
  "author": "Albert J. Wong",
  "license": "BSD-2-Clause",
  "keywords": [
    "iot"
  ],
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "firebase-tools": "^3.12.0",
    "foundation-sites": "^6.3.0",
    "material-components-web": "^0.22.0",
    "mocha": "^4.0.0",
    "node-sass": "^4.1.1",
    "resolve-url": "^0.2.1",
    "sass": "^1.0.0-beta.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "webpack": "^3.6.0"
  }
}
